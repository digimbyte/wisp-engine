{
  "name": "Retro Synthesizer",
  "version": "1.2.0",
  "author": "WispDev Studios",
  "description": "A vintage-style synthesizer with real-time audio effects",
  
  "audio": {
    "outputs": ["i2s", "bluetooth"],
    "sampleRate": 44100,
    "channels": 8,
    "streaming": true,
    "effects": true,
    "recording": false
  },
  
  "performance": {
    "fps": 24,
    "ram": 65536,
    "storage": 0
  },
  
  "system": {
    "wifi": false,
    "bluetooth": true,
    "eeprom": true
  },
  
  "entry": {
    "main": "synthesizer.cpp",
    "config": "audio_setup.h"
  }
}
